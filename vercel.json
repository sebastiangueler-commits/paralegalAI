{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}
